<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:niem-xs="http://release.niem.gov/niem/proxy/xsd/3.0/"
	xmlns:structures="http://release.niem.gov/niem/structures/3.0/"
	xmlns:nc="http://release.niem.gov/niem/niem-core/3.0/" xmlns:j="http://release.niem.gov/niem/domains/jxdm/5.1/"
	xmlns:ac-bkg-codes="http://ojbc.org/IEPD/Extensions/AdamsCounty/BookingCodes/1.0"
	xmlns:br-ext="http://ojbc.org/IEPD/Extensions/BookingReportExtension/1.0"
	targetNamespace="http://ojbc.org/IEPD/Extensions/AdamsCounty/BookingCodes/1.0"
	elementFormDefault="qualified">
	<xsd:import namespace="http://release.niem.gov/niem/niem-core/3.0/"
		schemaLocation="../../Subset/niem/niem-core/3.0/niem-core.xsd" />
	<xsd:import namespace="http://release.niem.gov/niem/domains/jxdm/5.1/"
		schemaLocation="../../Subset/niem/domains/jxdm/5.1/jxdm.xsd" />
	<xsd:import namespace="http://release.niem.gov/niem/proxy/xsd/3.0/"
		schemaLocation="../../Subset/niem/proxy/xsd/3.0/xs.xsd" />
	<xsd:import namespace="http://release.niem.gov/niem/structures/3.0/"
		schemaLocation="../../Subset/niem/structures/3.0/structures.xsd" />
	<xsd:import
		namespace="http://ojbc.org/IEPD/Extensions/BookingReportExtension/1.0"
		schemaLocation="../../extension.xsd" />
	<xsd:annotation>
		<xsd:documentation>Adams County Booking Codes</xsd:documentation>
	</xsd:annotation>
	<xsd:simpleType name="PreTrialCategoryCodeSimpleType">
		<xsd:annotation>
			<xsd:documentation>Pre Trial Category code set</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="SRP">
				<xsd:annotation>
					<xsd:documentation>Supervised Release Pretrial - under supervised
						released awaiting trial
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="C-PR">
				<xsd:annotation>
					<xsd:documentation>Cash Bond Pretrial - cash bond posted released
						awaiting trial
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="S-PR">
				<xsd:annotation>
					<xsd:documentation>Surety Bond Pretrial - surety bond posted
						released awaiting trial
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PR">
				<xsd:annotation>
					<xsd:documentation>Pretrial Release - released awaiting trial
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="C-SRP">
				<xsd:annotation>
					<xsd:documentation>Cash Bond Supervised Release Pretrial - cash
						bond posted released awaiting trial under supervision
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="S-SRP">
				<xsd:annotation>
					<xsd:documentation>Surety Bond Supervised Release Pretrial - surety
						bond posted released awaiting trial under supervision
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SUR">
				<xsd:annotation>
					<xsd:documentation>Surety - surety bond</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Split">
				<xsd:annotation>
					<xsd:documentation>bond split between cash and surety
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="No-Bond">
				<xsd:annotation>
					<xsd:documentation>In Jail No Bond Posted - in jail bond not posted
						or granted
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="PreTrialCategoryCodeType">
		<xsd:annotation>
			<xsd:documentation>Pre Trial Category code set</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="ac-bkg-codes:PreTrialCategoryCodeSimpleType">
				<xsd:attributeGroup ref="structures:SimpleObjectAttributeGroup" />
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:element name="PreTrialCategoryCode" type="ac-bkg-codes:PreTrialCategoryCodeType"
		substitutionGroup="nc:StatusAugmentationPoint">
		<xsd:annotation>
			<xsd:documentation>A category code for pretrial status.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- Need Definitions as noted below -->
	<xsd:simpleType name="BedCategoryCodeSimpleType">
		<xsd:annotation>
			<xsd:documentation>Bed Category code set</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Type 1">
				<xsd:annotation>
					<xsd:documentation>NEED DEFINITION</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Type 2">
				<xsd:annotation>
					<xsd:documentation>NEED DEFINITION</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Type 3">
				<xsd:annotation>
					<xsd:documentation>NEED DEFINITION</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Type 4">
				<xsd:annotation>
					<xsd:documentation>NEED DEFINITION</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Type 5">
				<xsd:annotation>
					<xsd:documentation>NEED DEFINITION</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other">
				<xsd:annotation>
					<xsd:documentation>Other Bad Category</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="BedCategoryCodeType">
		<xsd:annotation>
			<xsd:documentation>Pre Trial Category code set</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="ac-bkg-codes:BedCategoryCodeSimpleType">
				<xsd:attributeGroup ref="structures:SimpleObjectAttributeGroup" />
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:element name="BedCategoryCode" type="ac-bkg-codes:BedCategoryCodeType"
		substitutionGroup="nc:IdentificationAugmentationPoint">
		<xsd:annotation>
			<xsd:documentation>A category code for bed.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:simpleType name="MilitaryServiceStatusCodeSimpleType">
		<xsd:annotation>
			<xsd:documentation>Military Service Status code set
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="ACT">
				<xsd:annotation>
					<xsd:documentation>Active</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AWOL">
				<xsd:annotation>
					<xsd:documentation>AWOL</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DHD">
				<xsd:annotation>
					<xsd:documentation>Dishonorable Discharge</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="HD">
				<xsd:annotation>
					<xsd:documentation>Honorable Discharge</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MD">
				<xsd:annotation>
					<xsd:documentation>Medical Discharge</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NACT">
				<xsd:annotation>
					<xsd:documentation>Non Active</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="RET">
				<xsd:annotation>
					<xsd:documentation>Retired</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="MilitaryServiceStatusCodeType">
		<xsd:annotation>
			<xsd:documentation>Military Service Status code set
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="ac-bkg-codes:MilitaryServiceStatusCodeSimpleType">
				<xsd:attributeGroup ref="structures:SimpleObjectAttributeGroup" />
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:element name="MilitaryServiceStatusCode"
		type="ac-bkg-codes:MilitaryServiceStatusCodeType" substitutionGroup="nc:MilitarySummaryAugmentationPoint">
		<xsd:annotation>
			<xsd:documentation>A status code for military service.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:simpleType name="PersonRaceCodeSimpleType">
		<xsd:annotation>
			<xsd:documentation>Military Service Status code set
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="A">
				<xsd:annotation>
					<xsd:documentation>Asian or Pacific Islander</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="B">
				<xsd:annotation>
					<xsd:documentation>Black</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="I">
				<xsd:annotation>
					<xsd:documentation>American Indian or Alaskan Native
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="U">
				<xsd:annotation>
					<xsd:documentation>Unknown</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="W">
				<xsd:annotation>
					<xsd:documentation>White</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="P">
				<xsd:annotation>
					<xsd:documentation>Native Hawaiian or Pacific Islander
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="PersonRaceCodeType">
		<xsd:annotation>
			<xsd:documentation>Person Race code set
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="ac-bkg-codes:PersonRaceCodeSimpleType">
				<xsd:attributeGroup ref="structures:SimpleObjectAttributeGroup" />
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:element name="PersonRaceCode" type="ac-bkg-codes:PersonRaceCodeType"
		substitutionGroup="nc:PersonRace">
		<xsd:annotation>
			<xsd:documentation>A code for person race.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
</xsd:schema>