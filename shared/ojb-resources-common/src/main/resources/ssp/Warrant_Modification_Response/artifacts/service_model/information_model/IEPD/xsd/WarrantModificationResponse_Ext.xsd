<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:niem-xs="http://release.niem.gov/niem/proxy/xsd/3.0/"
	xmlns:structures="http://release.niem.gov/niem/structures/3.0/"
	xmlns:nc="http://release.niem.gov/niem/niem-core/3.0/" xmlns:appinfo="http://release.niem.gov/niem/appinfo/3.0/"
	xmlns:j="http://release.niem.gov/niem/domains/jxdm/5.1/"
	xmlns:wm-resp-ext="http://ojbc.org/IEPD/Extensions/WarrantModificationResponseExtension/1.0"
	targetNamespace="http://ojbc.org/IEPD/Extensions/WarrantModificationResponseExtension/1.0"
	elementFormDefault="qualified">
	<xsd:import namespace="http://release.niem.gov/niem/proxy/xsd/3.0/"
		schemaLocation="./Subset/niem/proxy/xsd/3.0/xs.xsd" />
	<xsd:import namespace="http://release.niem.gov/niem/structures/3.0/"
		schemaLocation="./Subset/niem/structures/3.0/structures.xsd" />
	<xsd:import namespace="http://release.niem.gov/niem/niem-core/3.0/"
		schemaLocation="./Subset/niem/niem-core/3.0/niem-core.xsd" />
	<xsd:import namespace="http://release.niem.gov/niem/domains/jxdm/5.1/"
		schemaLocation="./Subset/niem/domains/jxdm/5.1/jxdm.xsd" />
	<xsd:complexType name="WarrantModificationResponseType">
		<xsd:annotation>
			<xsd:documentation>A data type for a warrant modification response.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:ObjectType">
				<xsd:sequence>
					<xsd:element ref="j:Warrant" />
					<xsd:element ref="nc:Person" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="WarrantAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about a
				warrant.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="wm-resp-ext:StateWarrantRepositoryIdentification"
						minOccurs="0" />
					<xsd:element ref="wm-resp-ext:WarrantModificationAcceptedIndicator"
						minOccurs="1" />
					<xsd:element ref="wm-resp-ext:ModifiedWarrantNumber"
						minOccurs="0" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="StateWarrantRepositoryIdentification"
		type="nc:IdentificationType">
		<xsd:annotation>
			<xsd:documentation>A unique identification assigned to a warrant
				record.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="WarrantAugmentation" type="wm-resp-ext:WarrantAugmentationType"
		substitutionGroup="j:WarrantAugmentationPoint">
		<xsd:annotation>
			<xsd:documentation>Additional information about a warrant.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="WarrantModificationAcceptedIndicator"
		type="niem-xs:boolean">
		<xsd:annotation>
			<xsd:documentation>True if a warrant modification request has been
				accepted/approved; false otherwise.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ModifiedWarrantNumber" type="nc:IdentificationType">
		<xsd:annotation>
			<xsd:documentation>A unique number assigned to a modified warrant.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
</xsd:schema>
