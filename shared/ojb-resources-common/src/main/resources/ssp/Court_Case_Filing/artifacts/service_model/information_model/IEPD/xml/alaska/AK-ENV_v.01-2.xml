					<!--This action code should result in a Misdeameanor Case Type -->
					<nc:IdentificationID>AS2835285D1</nc:IdentificationID>
					<j:IdentificationJurisdictionNCICLISCode>AK
					</j:IdentificationJurisdictionNCICLISCode>
				</j:StatuteCodeSectionIdentification>
				<!--Required for the exchange -->
				<j:StatuteDescriptionText>1AS28.35.285(d)(1): Minors Refusal To
									Submit To Chemical Test/ 1st Offense</j:StatuteDescriptionText>
				<j:StatuteOffenseIdentification>
