					<!--Required for the exchange -->
					<!--Maps to REAL_CASE;ACTN_CD and PTYCHRG;ACTN_CD -->
					<nc:IdentificationID>AS2835285D1</nc:IdentificationID>
				</j:StatuteOffenseIdentification>
