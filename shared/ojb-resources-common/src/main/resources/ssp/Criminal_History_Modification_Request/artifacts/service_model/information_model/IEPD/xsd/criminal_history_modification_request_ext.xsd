<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:appinfo="http://release.niem.gov/niem/appinfo/4.0/"
	xmlns:structures="http://release.niem.gov/niem/structures/4.0/" xmlns:niem-xs="http://release.niem.gov/niem/proxy/xsd/4.0/"
	xmlns:nc="http://release.niem.gov/niem/niem-core/4.0/" xmlns:j="http://release.niem.gov/niem/domains/jxdm/6.0/"
	xmlns:chmr-ext="http://ojbc.org/IEPD/Extensions/CriminalHistoryModificationRequest/1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	targetNamespace="http://ojbc.org/IEPD/Extensions/CriminalHistoryModificationRequest/1.0" elementFormDefault="qualified">
	<xsd:import namespace="http://release.niem.gov/niem/appinfo/4.0/"
		schemaLocation="Subset/niem/utility/appinfo/4.0/appinfo.xsd" />
	<xsd:import namespace="http://release.niem.gov/niem/structures/4.0/"
		schemaLocation="Subset/niem/utility/structures/4.0/structures.xsd" />
	<xsd:import namespace="http://release.niem.gov/niem/proxy/xsd/4.0/" schemaLocation="Subset/niem/proxy/xsd/4.0/xs.xsd" />
	<xsd:import namespace="http://release.niem.gov/niem/niem-core/4.0/"
		schemaLocation="Subset/niem/niem-core/4.0/niem-core.xsd" />
	<xsd:import namespace="http://release.niem.gov/niem/domains/jxdm/6.0/"
		schemaLocation="Subset/niem/domains/jxdm/6.0/jxdm.xsd" />
	<xsd:complexType name="CriminalHistoryModificationRequestType">
		<xsd:annotation>
			<xsd:documentation>A data type for a criminal history modification request.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:ObjectType">
				<xsd:sequence>
					<xsd:choice>
						<xsd:element ref="chmr-ext:ArrestAddRequest" minOccurs="0" maxOccurs="unbounded" />
						<xsd:element ref="chmr-ext:ArrestModifyRequest" minOccurs="0" maxOccurs="unbounded" />
						<xsd:element ref="chmr-ext:ArrestDeleteRequest" minOccurs="0" maxOccurs="unbounded" />
						<xsd:element ref="chmr-ext:DispositionAddRequest" minOccurs="0" maxOccurs="unbounded" />
						<xsd:element ref="chmr-ext:DispositionDeleteRequest" minOccurs="0" maxOccurs="unbounded" />
					</xsd:choice>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ArrestAddRequestType">
		<xsd:annotation>
			<xsd:documentation>A data type for an add arrest request.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:ObjectType">
				<xsd:sequence>
					<xsd:element ref="j:Arrest" minOccurs="0" maxOccurs="1" />
					<xsd:element ref="j:Subject" minOccurs="0" maxOccurs="1" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ArrestModifyRequestType">
		<xsd:annotation>
			<xsd:documentation>A data type for a modify arrest request.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:ObjectType">
				<xsd:sequence>
					<xsd:element ref="j:Arrest" minOccurs="0" maxOccurs="1" />
					<xsd:element ref="j:Subject" minOccurs="0" maxOccurs="1" />
					<xsd:element ref="j:Charge" minOccurs="0" maxOccurs="1" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ArrestDeleteRequestType">
		<xsd:annotation>
			<xsd:documentation>A data type for a delete arrest request.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:ObjectType">
				<xsd:sequence>
					<xsd:element ref="j:Arrest" minOccurs="0" maxOccurs="1" />
					<xsd:element ref="j:Subject" minOccurs="0" maxOccurs="1" />
					<xsd:element ref="j:Charge" minOccurs="0" maxOccurs="1" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DispositionRequestType">
		<xsd:annotation>
			<xsd:documentation>A data type for a add disposition request.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:ObjectType">
				<xsd:sequence>
					<xsd:element ref="nc:Disposition" minOccurs="0" maxOccurs="1" />
					<xsd:element ref="j:Sentence" minOccurs="0" maxOccurs="1" />
					<xsd:element ref="j:Restitution" minOccurs="0" maxOccurs="1" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="ArrestAddRequest" type="chmr-ext:ArrestAddRequestType">
		<xsd:annotation>
			<xsd:documentation>A request to add an arrest record.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ArrestModifyRequest" type="chmr-ext:ArrestModifyRequestType">
		<xsd:annotation>
			<xsd:documentation>A request to modify (edit) an arrest record.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ArrestDeleteRequest" type="chmr-ext:ArrestDeleteRequestType">
		<xsd:annotation>
			<xsd:documentation>A request to delete an arrest record.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DispositionAddRequest" type="chmr-ext:DispositionRequestType">
		<xsd:annotation>
			<xsd:documentation>A request to add a disposition record.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DispositionDeleteRequest" type="chmr-ext:DispositionRequestType">
		<xsd:annotation>
			<xsd:documentation>A request to delete a disposition record.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DispositionChargeStatute" type="j:StatuteType" substitutionGroup="nc:DispositionAugmentationPoint">
		<xsd:annotation>
			<xsd:documentation>A unique identifier of a law, rule, or ordinance within a jurisdiction that a person is accused of violating.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DispositionProvisionCodeText" type="nc:TextType" substitutionGroup="nc:DispositionAugmentationPoint">
		<xsd:annotation>
			<xsd:documentation>A disposition condition code text.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DispositionProvisionDescriptionText" type="nc:TextType"
		substitutionGroup="nc:DispositionAugmentationPoint">
		<xsd:annotation>
			<xsd:documentation>A description of a disposition condition.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
</xsd:schema>