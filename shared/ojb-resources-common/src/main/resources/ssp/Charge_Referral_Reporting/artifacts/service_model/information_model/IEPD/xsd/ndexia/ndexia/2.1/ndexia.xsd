					<xsd:element ref="ndexia:ModelYearRange" minOccurs="0"/>
					<xsd:element ref="ndexia:VehicleStyleCategoryCode" minOccurs="0"/>
					<xsd:element ref="ndexia:StateAssignedVINIndicator" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="VehicleType">
		<xsd:annotation>
			<xsd:documentation>A data type for a motor-driven conveyance designed to carry its operator, and optionally passengers and cargo, over land.  Builds upon a LEXS Digest Vehicle.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="VehicleType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="nc:VehicleType">
				<xsd:sequence>
					<xsd:element ref="ndexia:VehicleAugmentation"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="VesselAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type that supplements nc:VesselType.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://fbi.gov/cjis/N-DEx/IncidentArrest/2.1" i:name="AugmentationType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ndexia:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ndexia:ConveyanceFuelCategoryCode" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:ConveyanceRegistration" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:EngineDisplacementMeasure" minOccurs="0"/>
					<xsd:element ref="ndexia:ItemLotNumberText" minOccurs="0"/>
					<xsd:element ref="ndexia:ItemQuantityStatusValue" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:MerchandiseIndicator" minOccurs="0"/>
					<xsd:element ref="ndexia:VesselCategoryText" minOccurs="0"/>
					<xsd:element ref="ndexia:VesselHomePortText" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="VesselType">
		<xsd:annotation>
			<xsd:documentation>A data type for a mode of transportation capable of transport by water.  Builds upon a LEXS Digest Vessel.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="VesselType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="nc:VesselType">
				<xsd:sequence>
					<xsd:element ref="ndexia:VesselAugmentation"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="VictimAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type that supplements j:VictimType.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://fbi.gov/cjis/N-DEx/IncidentArrest/2.1" i:name="AugmentationType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ndexia:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="j:VictimAggravatedAssaultHomicideFactorText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:VictimAggravatedAssaultHomicideFactorCode" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:VictimMOActionCode" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:VictimMOActionText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:VictimMOApproachedCode" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:VictimMOApproachedText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:VictimMOBindingsCode" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:VictimMOBindingsText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:VictimMOFoundClothedCode" minOccurs="0"/>
					<xsd:element ref="ndexia:VictimMOFoundClothedDescriptionText" minOccurs="0"/>
					<xsd:element ref="ndexia:VictimMOFoundClothedText" minOccurs="0"/>
					<xsd:element ref="ndexia:VictimMOGeneralLifestyleCode" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:VictimMOGeneralLifestyleText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:VictimMORecoveryCode" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:VictimMORecoveryDescriptionText" minOccurs="0"/>
					<xsd:element ref="ndexia:VictimMORecoveryText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:VictimMOReleaseDisposalCode" minOccurs="0"/>
					<xsd:element ref="ndexia:VictimMOReleaseDisposalText" minOccurs="0"/>
					<xsd:element ref="ndexia:VictimMOSubjectContactConclusionCode" minOccurs="0"/>
					<xsd:element ref="ndexia:VictimMOSubjectContactConclusionText" minOccurs="0"/>
					<xsd:element ref="ndexia:VictimSequenceNumberText" minOccurs="0"/>
					<xsd:element ref="ndexia:VictimVICAPCaseNumberText" minOccurs="0"/>
					<xsd:element ref="ndexia:VictimVICAPIndicator" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="VictimType">
		<xsd:annotation>
			<xsd:documentation>A data type for a person or organization who suffers injury, loss, or death as a result of an activity.  Builds upon a LEXS Digest Victim.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/domains/jxdm/4.0" i:name="VictimType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="j:VictimType">
				<xsd:sequence>
					<xsd:element ref="ndexia:VictimAugmentation"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="WarrantType">
		<xsd:annotation>
			<xsd:documentation>A data type for an authorization for an enforcement official to perform a specified action.  Builds upon a LEXS Digest Activity.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/domains/jxdm/4.0" i:name="WarrantType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="j:WarrantType">
				<xsd:sequence>
					<xsd:element ref="ndexia:WarrantAugmentation"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="WitnessAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type that supplements j:WitnessType.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://fbi.gov/cjis/N-DEx/IncidentArrest/2.1" i:name="AugmentationType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ndexia:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ndexia:WitnessSequenceNumberText" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="WitnessType">
		<xsd:annotation>
			<xsd:documentation>A data type for a person who has observed an activity.  Builds upon a LEXS Digest Witness.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/domains/jxdm/4.0" i:name="WitnessType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="j:WitnessType">
				<xsd:sequence>
					<xsd:element ref="ndexia:WitnessAugmentation"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="YearRangeType">
		<xsd:annotation>
			<xsd:documentation>A data type for a range of years.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ComplexObjectType">
				<xsd:sequence>
					<xsd:element ref="ndexia:EndYear" minOccurs="0"/>
					<xsd:element ref="ndexia:StartYear" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ================================================================== -->
	<!-- Entity Elements -->
	<!-- ================================================================== -->
	<!-- These elements are all substitutable for the N-DEx Entity object, so that all high level objects that may appear in a report are grouped together without having to be in a specific order. -->
	<!-- ================================================================== -->
	<xsd:element name="Aircraft" type="ndexia:AircraftType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A mode of transportation capable of flying in the air.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Arrest" type="ndexia:ArrestType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A set of details about action taken by law enforcement to detain an individual with the intention of seeking charges against the person for a specific offense(s) and a record is made of the detention, includes juveniles taken into custody but merely warned and released without being charged.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ArrestOfficial" type="ndexia:EnforcementOfficialType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person involved in the enforcement of law who made an arrest. This generally refers to a person designated by a public authority to keep the peace and arrest persons guilty or suspected of crime. May include, but not limited to: police, sheriff, marshal, federal authorities, or military.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ArrestSubject" type="ndexia:ArrestSubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person who is arrested for a violation of a criminal statute, ordinance or rule.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Attachment" type="ndexia:AttachmentType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A binary, such as an image or PDF file or video, as well as information about the binary, such as the size and type and description.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BailSubject" type="ndexia:SubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person that is involved or suspected of being involved in a violation of a criminal statute, ordinance or rule who has been assigned bail.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BookingEmployee" type="ndexia:EnforcementOfficialType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person involved in the enforcement of law who booked a subject. This generally refers to a person designated by a public authority to keep the peace and arrest persons guilty or suspected of crime. May include, but not limited to: police, sheriff, marshal, federal authorities, or military.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BookingReleaseCorrectionsAnalyst" type="ndexia:EnforcementOfficialType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A corrections pre-trial screening official.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BookingSearchOfficial" type="ndexia:EnforcementOfficialType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An official who searches a subject during a booking.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BookingSubject" type="ndexia:SubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person that is involved or suspected of being involved in a violation of a criminal statute, ordinance or rule who has been booked.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BookingTelephoneCallSupervisingOfficial" type="ndexia:EnforcementOfficialType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An official who supervises a subject making a telephone call.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BookingTransportOfficial" type="ndexia:EnforcementOfficialType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An official who supervises a subject making a telephone call.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CaseWitness" type="ndexia:WitnessType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person who has information relating to the activity or who testifies in court.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ChargeSubject" type="ndexia:SubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person that is involved or suspected of being involved in a violation of a criminal statute, ordinance or rule who has been charged.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ChargeVictim" type="ndexia:VictimType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person or organization that was negatively affected in an incident as designated by the charge.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CitationIssuingOfficial" type="ndexia:EnforcementOfficialType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An official who gives a citation to a subject.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CitationSubject" type="ndexia:SubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person that is involved or suspected of being involved in a violation of a criminal statute, ordinance or rule who has been cited.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Computer" type="ndexia:ComputerType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An electronic device that processes information based on a set of instructions.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ConveyancePart" type="ndexia:ConveyancePartType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A part or component of a boat, vehicle, or aircraft.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ConvictionSubject" type="ndexia:SubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person that is involved or suspected of being involved in a violation of a criminal statute, ordinance or rule who has been convicted.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CourtOrderDesignatedSubject" type="ndexia:SubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person that is involved or suspected of being involved in a violation of a criminal statute, ordinance or rule to whom a court order applies.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CourtOrderServiceOfficialEnforcement" type="ndexia:EnforcementOfficialType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An official who delivered a court order.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CustodyTransferReceivingEnforcementOfficial" type="ndexia:EnforcementOfficialType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An official that receives custody of a subject or property.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CustodyTransferReleasingEnforcementOfficial" type="ndexia:EnforcementOfficialType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An official that releases custody of a subject or property.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CustodyTransferSubject" type="ndexia:SubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person that is involved or suspected of being involved in a violation of a criminal statute, ordinance or rule who is being transferred from one custody to another.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Drug" type="ndexia:DrugType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A legal or illegal substance that can be used as a medicine or a narcotic.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EnforcementOfficial" type="ndexia:EnforcementOfficialType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person involved in the enforcement of law. This generally refers to a person designated by a public authority to keep the peace and arrest persons guilty or suspected of crime. May include, but not limited to: police, sheriff, marshal, federal authorities, or military.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EnforcementUnit" type="ndexia:EnforcementUnitType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A unit of an agency responsible for enforcing the law and maintaining peace.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Evidence" type="ndexia:EvidenceType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A role of property legally received by or submitted to an agency for use in ascertaining the truth of a matter.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Explosive" type="ndexia:ExplosiveType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A device containing a substance or agent that can cause an explosion.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FingerprintImage" type="ndexia:FingerprintImageType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An image of a fingerprint of a person.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Firearm" type="ndexia:FirearmType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A weapon that will, or is designed to, or may readily be converted to expel a projectile by air, carbon dioxide, or the action of an explosive (excluding BB guns).</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ForceSubject" type="ndexia:SubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person that is involved or suspected of being involved in a violation of a criminal statute, ordinance or rule who used force.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ForceVictim" type="ndexia:VictimType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person or organization that was negatively affected in an incident, and who was a victim of force.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ForensicSpecimen" type="ndexia:ForensicSpecimenType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A specimen collected for the purpose of analysis which may be of use as evidence in criminal, civil, judicial, or administrative proceedings.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="HazardousMaterial" type="ndexia:SubstanceType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An item or agent (biological, chemical, or physical) which has the potential to cause harm to humans, animals, or the environment, either by itself or through interaction with other factors.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IAFISIndicator" type="niem-xsd:boolean" nillable="true">
		<xsd:annotation>
			<xsd:documentation>True if IAFIS was able to make an identification based upon the submitted transaction; false if it did not.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Incident" type="ndexia:IncidentType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An occurrence or event requiring response, often involving one or more offenses committed by the same offender, or group of offenders acting in concert, at the same time and place.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IncidentAssistingOfficial" type="ndexia:EnforcementOfficialType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An official that assisted in processing an incident.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IncidentEvidence" type="ndexia:EvidenceType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A role of property legally received by or submitted to an agency for use in ascertaining the truth of a matter about an incident.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IncidentReportingOfficial" type="ndexia:EnforcementOfficialType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An official that submitted an incident report in an incident.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IncidentResponseOfficial" type="ndexia:EnforcementOfficialType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An official that responded to an incident.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IncidentSubject" type="ndexia:SubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person that is involved or suspected of being involved in a violation of a criminal statute, ordinance or rule whose actions caused an incident.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IncidentSupervisingOfficial" type="ndexia:EnforcementOfficialType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An enforcement supervisor responsible for, or present at, an incident.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IncidentVictim" type="ndexia:VictimType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person or organization that was negatively affected in an incident.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IncidentWitness" type="ndexia:WitnessType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person who has observed an incident.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Inmate" type="ndexia:SubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person who is or has been incarcerated.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IntangibleItem" type="ndexia:IntangibleItemType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A piece of property which cannot be perceived by the sense of touch, excluding intellectual property.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IntellectualProperty" type="ndexia:IntellectualPropertyType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A right or privilege originating from original thought or creativity normally granted through formalized registry.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Jewelry" type="ndexia:JewelryType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An adornment made out of metals, gems, or imitation materials.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="JewelryStone" type="ndexia:JewelryStoneType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A piece of attractive mineral, which - when cut and polished - is used to make jewelry or other adornments, also referred to as gemstone, gem, or precious or semi-precious stone.  Jewelry Stones may be a part of a piece of jewelry.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Location" type="ndexia:LocationType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A physical location.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Metal" type="ndexia:MetalType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A metallic chemical element. </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MissingPersonLastSeenWitness" type="ndexia:WitnessType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person who last saw a missing person.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MissingPersonOccurrence" type="ndexia:MissingPersonOccurrenceType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A set of details surrounding the activity perpetuated by a person's whereabouts being unknown.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Offense" type="ndexia:OffenseType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A violation of a criminal statute, ordinance or rule.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Organization" type="ndexia:OrganizationType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A unit which conducts some sort of business or operations.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Parolee" type="ndexia:SubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person who has been paroled.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Person" type="ndexia:PersonType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A human being.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PreciousMetal" type="ndexia:PreciousMetalType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A rare metallic chemical element of high economic value, typically regarded as an investment and industrial commodity. </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Prisoner" type="ndexia:SubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person incarcerated in a facility or otherwise remanded to or kept in custody.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Probationer" type="ndexia:SubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person who is on probation.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PropertySeizureSeizingEnforcementOfficial" type="ndexia:EnforcementOfficialType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An official that seized a property item.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Publication" type="ndexia:PublicationType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A collection of written, printed, illustrated, or blank sheets, made of paper, parchment, or other material, usually fastened together to hinge at one side.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RegisteredOffender" type="ndexia:RegisteredOffenderType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person that is required to register their residential information with a local law enforcement agency due to having been convicted of a certain kind of crime.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Securities" type="ndexia:SecuritiesType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A financial instrument showing ownership or credit.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SentenceSubject" type="ndexia:SubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person who has been sentenced for being involved in a violation of a criminal statute, ordinance or rule who used force.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ServiceCall" type="ndexia:ServiceCallType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A call for service received from a person alerting an authority of a situation.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ServiceCallDispatchedOfficial" type="ndexia:EnforcementOfficialType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An official assigned specifically to handle a call for service.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Structure" type="ndexia:StructureType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A building or construction (e.g. built environment, nonbuilding structure, etc).</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Subject" type="ndexia:SubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person that is involved or suspected of being involved in a violation of a criminal statute, ordinance or rule.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SupervisionSubject" type="ndexia:SubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person that is involved or suspected of being involved in a violation of a criminal statute, ordinance or rule who is under supervision.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Suspect" type="ndexia:SubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person that is suspected of being involved in a violation of a criminal statute, ordinance or rule.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TangibleItem" type="ndexia:TangibleItemType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A physical object.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TelecommunicationDevice" type="ndexia:TelecommunicationDeviceType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An electronic device used for communication over a distance.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Tool" type="ndexia:ToolType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A role of property used to facilitate commission of a crime.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Vehicle" type="ndexia:VehicleType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A motor-driven conveyance designed to carry its operator, and optionally passengers and cargo, over land.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="VerdictSubject" type="ndexia:SubjectType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person that is involved or suspected of being involved in a violation of a criminal statute, ordinance or rule who received a verdict.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Vessel" type="ndexia:VesselType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A mode of transportation capable of transport by water.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Victim" type="ndexia:VictimType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person or organization that was negatively affected in an activity.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="VisitationSupervisingOfficialEnforcement" type="ndexia:EnforcementOfficialType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An official who supervised a visit.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Warrant" type="ndexia:WarrantType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>An authorization for an enforcement official to perform a specified action.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Witness" type="ndexia:WitnessType" substitutionGroup="ndexia:Entity" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A person who has observed an activity.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- ================================================================== -->
	<!-- Association Elements-->
	<!-- ================================================================== -->
	<!-- These elements are all substitutable for the N-DEx Association object, so that all associations that may appear in a report are grouped together without having to be in a specific order. -->
	<!-- ================================================================== -->
	<xsd:element name="FinancialSupporterAssociation" type="ndexia:FinancialSupporterAssociationType" substitutionGroup="ndexia:Association">
		<xsd:annotation>
			<xsd:documentation>A relationship between a person or organization who receives financial support and a person or an organization that provides financial support or assistance.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DomesticPartnershipAssociation" type="ndexia:PersonUnionAssociationType" substitutionGroup="ndexia:Association">
		<xsd:annotation>
			<xsd:documentation>A relationship between two people who live together, are of the same sex, and share a significant, spouse-like relationship without the partnership being legally sanctioned.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IntimacyAssociation" type="ndexia:IntimacyAssociationType" substitutionGroup="ndexia:Association">
		<xsd:annotation>
			<xsd:documentation>A relationship between people based on the presence of sexual/sexually romantic activity.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MarriageAssociation" type="ndexia:PersonUnionAssociationType" substitutionGroup="ndexia:Association">
		<xsd:annotation>
			<xsd:documentation>A relationship between two people who are legally married to each other.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="OrganizationThreatGroupAssociation" type="ndexia:OrganizationAssociationType" substitutionGroup="ndexia:Association">
		<xsd:annotation>
			<xsd:documentation>A relationship between an organization and an organization which is considered a threat group.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PersonCriminalOrganizationAssociation" type="ndexia:PersonThreatGroupAssociationType" substitutionGroup="ndexia:Association">
		<xsd:annotation>
			<xsd:documentation>A relationship between a person and a criminal organization.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PersonGangAssociation" type="ndexia:PersonThreatGroupAssociationType" substitutionGroup="ndexia:Association">
		<xsd:annotation>
			<xsd:documentation>A relationship between a person and a gang.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PersonStolenIdentityAssociation" type="ndexia:PersonStolenIdentityAssociationType" substitutionGroup="ndexia:Association">
		<xsd:annotation>
			<xsd:documentation>A relationship between a person and a stolen identity used by that person.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PersonThreatGroupAssociation" type="ndexia:PersonThreatGroupAssociationType" substitutionGroup="ndexia:Association">
		<xsd:annotation>
			<xsd:documentation>A relationship between a person and any organization, i.e., club, association or group of individuals, either formal or informal (including traditional gangs and terrorist organizations), that may have a common name or identifying symbol, and whose members engage in activities that include, but are not limited to planning, organizing, threatening, financing, soliciting, committing or attempting to commit acts that would violate the law, which detract from the safe and orderly operations of society.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SubjectVictimAssociation" type="ndexia:SubjectVictimAssociationType" substitutionGroup="ndexia:Association">
		<xsd:annotation>
			<xsd:documentation>A relationship between a subject and a victim.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
