<xsd:schema xmlns:ansi-nist="http://niem.gov/niem/ansi-nist/2.0" xmlns:fbi="http://niem.gov/niem/fbi/2.0" xmlns:i="http://niem.gov/niem/appinfo/2.0" xmlns:fips_10-4="http://niem.gov/niem/fips_10-4/2.0" xmlns:j="http://niem.gov/niem/domains/jxdm/4.0" xmlns:lexs="http://usdoj.gov/leisp/lexs/3.1" xmlns:lexsdigest="http://usdoj.gov/leisp/lexs/digest/3.1" xmlns:lexslib="http://usdoj.gov/leisp/lexs/library/3.1" xmlns:mmucc_2="http://niem.gov/niem/mmucc_2/2.0" xmlns:nc="http://niem.gov/niem/niem-core/2.0" xmlns:ndexia="http://fbi.gov/cjis/N-DEx/IncidentArrest/2.1" xmlns:ndexiacodes="http://fbi.gov/cjis/N-DEx/IncidentArrest/codes/2.1" xmlns:niem-xsd="http://niem.gov/niem/proxy/xsd/2.0" xmlns:s="http://niem.gov/niem/structures/2.0" xmlns:scr="http://niem.gov/niem/domains/screening/2.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://fbi.gov/cjis/N-DEx/IncidentArrest/2.1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.1.1">
	<xsd:import namespace="http://niem.gov/niem/structures/2.0" schemaLocation="../../../NIEM_2.0/structures/2.0/structures.xsd"/>
	<xsd:import namespace="http://niem.gov/niem/appinfo/2.0" schemaLocation="../../../NIEM_2.0/appinfo/2.0/appinfo.xsd"/>
	<xsd:import namespace="http://niem.gov/niem/ansi-nist/2.0" schemaLocation="../../../NIEM_2.0/ansi-nist/2.0/ansi-nist.xsd"/>
	<xsd:import namespace="http://niem.gov/niem/niem-core/2.0" schemaLocation="../../../NIEM_2.0/niem-core/2.0/niem-core.xsd"/>
	<xsd:import namespace="http://niem.gov/niem/domains/jxdm/4.0" schemaLocation="../../../NIEM_2.0/domains/jxdm/4.0/jxdm.xsd"/>
	<xsd:import namespace="http://niem.gov/niem/proxy/xsd/2.0" schemaLocation="../../../NIEM_2.0/proxy/xsd/2.0/xsd.xsd"/>
	<xsd:import namespace="http://niem.gov/niem/mmucc_2/2.0" schemaLocation="../../../NIEM_2.0/mmucc_2/2.0/mmucc_2.xsd"/>
	<xsd:import namespace="http://niem.gov/niem/fbi/2.0" schemaLocation="../../../NIEM_2.0/fbi/2.0/fbi.xsd"/>
	<xsd:import namespace="http://niem.gov/niem/fips_10-4/2.0" schemaLocation="../../../NIEM_2.0/fips_10-4/2.0/fips_10-4.xsd"/>
	<xsd:import namespace="http://niem.gov/niem/domains/screening/2.0" schemaLocation="../../../NIEM_2.0/domains/screening/2.0/screening.xsd"/>
	<xsd:import namespace="http://fbi.gov/cjis/N-DEx/IncidentArrest/codes/2.1" schemaLocation="../../ndexia-codes/2.1/ndexia-codes.xsd"/>
	<xsd:import namespace="http://usdoj.gov/leisp/lexs/library/3.1" schemaLocation="../../../lexs/library/3.1/library.xsd">
		<xsd:annotation>
			<xsd:documentation>Library for common LEXS elements</xsd:documentation>
			<xsd:appinfo>
				<i:ConformantIndicator>false</i:ConformantIndicator>
			</xsd:appinfo>
		</xsd:annotation>
	</xsd:import>
	<xsd:import namespace="http://usdoj.gov/leisp/lexs/digest/3.1" schemaLocation="../../../lexs/digest/3.1/digest.xsd"/>
	<xsd:import namespace="http://usdoj.gov/leisp/lexs/3.1" schemaLocation="../../../lexs/lexs/3.1/lexs.xsd">
		<xsd:annotation>
			<xsd:documentation>LEXS Messages Structure Format Schema</xsd:documentation>
			<xsd:appinfo>
				<i:ConformantIndicator>false</i:ConformantIndicator>
			</xsd:appinfo>
		</xsd:annotation>
	</xsd:import>
	<xsd:annotation>
		<xsd:documentation>Community structures</xsd:documentation>
		<xsd:appinfo>
			<i:ConformantIndicator>true</i:ConformantIndicator>
		</xsd:appinfo>
	</xsd:annotation>
	<!-- ================================================================== -->
	<!-- Types -->
	<!-- ================================================================== -->
	<xsd:complexType name="ActivityStatusAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type that supplements nc:StatusType.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="AugmentationType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ndexia:IncidentStatusCode" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ActivityStatusType">
		<xsd:annotation>
			<xsd:documentation>A data type for a set of additional information for nc:StatusType in the LEXS digest.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="StatusType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="nc:StatusType">
				<xsd:sequence>
					<xsd:element ref="ndexia:ActivityStatusAugmentation" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ActivityType">
		<xsd:annotation>
			<xsd:documentation>A data type for a specialized activity in N-DEx.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="ActivityType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="nc:ActivityType">
				<xsd:sequence>
					<xsd:element ref="ndexia:ActivityAugmentation"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AircraftAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type that supplements nc:AircraftType.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://fbi.gov/cjis/N-DEx/IncidentArrest/2.1" i:name="AugmentationType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ndexia:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="nc:IdentificationJurisdiction" minOccurs="0"/>
					<xsd:element ref="ndexia:ConveyanceFuelCategoryCode" minOccurs="0"/>
					<xsd:element ref="ndexia:ConveyanceJointlyRegisteredIndicator" minOccurs="0"/>
					<xsd:element ref="ndexia:ItemLotNumberText" minOccurs="0"/>
					<xsd:element ref="ndexia:ItemQuantityStatusValue" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:MerchandiseIndicator" minOccurs="0"/>
					<xsd:element ref="ndexia:ModelYearRange" minOccurs="0"/>
					<xsd:element ref="ndexia:StateAssignedVINIndicator" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AircraftType">
		<xsd:annotation>
			<xsd:documentation>A data type for a mode of transportation capable of flying in the air.  Builds upon a LEXS digest Aircraft.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="AircraftType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="nc:AircraftType">
				<xsd:sequence>
					<xsd:element ref="ndexia:AircraftAugmentation"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ArrestAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type that supplements j:ArrestType.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://fbi.gov/cjis/N-DEx/IncidentArrest/2.1" i:name="AugmentationType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ndexia:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ndexia:ArrestCaseNumberText" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ArrestSubjectAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type that supplements j:SubjectType, specifically for ArrestSubjects.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://fbi.gov/cjis/N-DEx/IncidentArrest/2.1" i:name="AugmentationType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ndexia:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="j:ArrestSequenceID" minOccurs="0"/>
					<xsd:element ref="j:ArrestSubjectCountCode" minOccurs="0"/>
					<xsd:element ref="j:ChargeUCRCode" minOccurs="0"/>
					<xsd:element ref="j:Statute" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:ArresteeArmedWithCode" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:ArresteeClearanceIndicator" minOccurs="0"/>
					<xsd:element ref="ndexia:ArresteeJuvenileDispositionCode" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ArrestSubjectType">
		<xsd:annotation>
			<xsd:documentation>A data type for a person who is arrested for a violation of a criminal statute, ordinance or rule.  Builds upon an element of SubjectType in the LEXS Digest.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/domains/jxdm/4.0" i:name="SubjectType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="j:SubjectType">
				<xsd:sequence>
					<xsd:element ref="ndexia:ArrestSubjectAugmentation"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ArrestType">
		<xsd:annotation>
			<xsd:documentation>A data type for an action taken by law enforcement to detain an individual with the intention of seeking charges against the person for a specific offense(s) and a record is made of the detention, includes juveniles taken into custody but merely warned and released without being charged.  Builds upon a LEXS Digest Activity.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/domains/jxdm/4.0" i:name="ArrestType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="j:ArrestType">
				<xsd:sequence>
					<xsd:element ref="ndexia:ArrestAugmentation"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AttachmentType">
		<xsd:annotation>
			<xsd:documentation>A data type for a set of additional information for a LEXS Attachment.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ComplexObjectType">
				<xsd:sequence>
					<xsd:element ref="lexs:AttachmentURI"/>
					<xsd:element ref="ndexia:BinaryCategoryCode" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type that supplements top level N-DEx Entity elements. Type includes LEXS SameAsDigestReference so that each new augmentation type doesn't have to include it.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="AugmentationType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="lexslib:SameAsDigestReference"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ComputerAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type that supplements ndexia:ComputerType to add SameAsDigestReference.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://fbi.gov/cjis/N-DEx/IncidentArrest/2.1" i:name="AugmentationType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ndexia:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="lexsdigest:ElectronicAddressMACNumber" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:ItemLotNumberText" minOccurs="0"/>
					<xsd:element ref="ndexia:ItemQuantityStatusValue" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:MerchandiseIndicator" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ComputerType">
		<xsd:annotation>
			<xsd:documentation>A data type for an electronic device that processes information based on a set of instructions.  Builds upon a LEXS Digest TangibleItem.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="ComputerType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="nc:ComputerType">
				<xsd:sequence>
					<xsd:element ref="ndexia:ComputerAugmentation"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ConveyancePartAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type that supplements ndexia:ConveyancePartType to add SameAsDigestReference.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://fbi.gov/cjis/N-DEx/IncidentArrest/2.1" i:name="AugmentationType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ndexia:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ndexia:ItemLotNumberText" minOccurs="0"/>
					<xsd:element ref="ndexia:ItemQuantityStatusValue" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:MerchandiseIndicator" minOccurs="0"/>
					<xsd:element ref="ndexia:VehiclePartOrAccessoryIndicator" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ConveyancePartType">
		<xsd:annotation>
			<xsd:documentation>A data type for a part or component of a boat, vehicle, or aircraft.  Builds upon the LEXS Digest TangibleItem.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="ConveyancePartType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="nc:ConveyancePartType">
				<xsd:sequence>
					<xsd:element ref="ndexia:ConveyancePartAugmentation"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ConveyanceRegistrationAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type that supplements nc:ConveyanceRegistrationType.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://fbi.gov/cjis/N-DEx/IncidentArrest/2.1" i:name="AugmentationType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ndexia:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ndexia:ConveyanceJointlyRegisteredIndicator" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ConveyanceRegistrationType">
		<xsd:annotation>
			<xsd:documentation>A data type for a registration of a conveyance with an authority.  Adds LEXS SameAsDigestReference in order to link the registration information to the ConveyanceRegistrationPlateIdentification in the LEXS Digest.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="ConveyanceRegistrationType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="nc:ConveyanceRegistrationType">
				<xsd:sequence>
					<xsd:element ref="ndexia:ConveyanceRegistrationAugmentation" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DrugAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type that supplements nc:DrugType to add the LEXS SameAsDigestReference.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://fbi.gov/cjis/N-DEx/IncidentArrest/2.1" i:name="AugmentationType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ndexia:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ndexia:ItemLotNumberText" minOccurs="0"/>
					<xsd:element ref="ndexia:ItemQuantityStatusValue" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ndexia:MerchandiseIndicator" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DrugType">
		<xsd:annotation>
			<xsd:documentation>A data type for a legal or illegal substance that can be used as a medicine or a narcotic.  Builds upon a LEXS Digest Drug.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="DrugType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="nc:DrugType">
				<xsd:sequence>
					<xsd:element ref="ndexia:DrugAugmentation"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="EnforcementOfficialType">
		<xsd:annotation>
			<xsd:documentation>A data type for a person involved in the enforcement of law. This generally refers to a person designated by a public authority to keep the peace and arrest persons guilty or suspected of crime. May include, but not limited to: police, sheriff, marshal, federal authorities, or military.  Builds upon elements of j:EnforcementOfficialType in the LEXS Digest.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/domains/jxdm/4.0" i:name="EnforcementOfficialType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="j:EnforcementOfficialType">
				<xsd:sequence>
					<xsd:element ref="ndexia:EnforcementOfficialAugmentation"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="EnforcementUnitType">
		<xsd:annotation>
			<xsd:documentation>A data type for a unit of an agency responsible for enforcing the law and maintaining peace.  Builds upon a LEXS Digest Organization.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/domains/jxdm/4.0" i:name="EnforcementUnitType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="j:EnforcementUnitType">
				<xsd:sequence>
					<xsd:element ref="j:OrganizationAugmentation" minOccurs="0"/>
					<xsd:element ref="ndexia:EnforcementUnitAugmentation"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="EngineDisplacementMeasureType">
		<xsd:annotation>
			<xsd:documentation>A data type for a measurement of the displacement of a conveyance engine.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="MeasureType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="nc:MeasureType">
				<xsd:sequence>
					<xsd:element ref="ndexia:EngineDisplacementUnitCode" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="EvidenceType">
		<xsd:annotation>
			<xsd:documentation>A data type for a role of property legally received by or submitted to an agency for use in ascertaining the truth of a matter.  Note that this is not a role in NIEM, so is created from scratch here rather than being based on NIEM's EvidenceType.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ComplexObjectType">
				<xsd:sequence>
					<xsd:element ref="nc:RoleOfItemReference"/>
					<xsd:element ref="j:EvidenceReceiptIdentification" minOccurs="0"/>
					<xsd:element ref="ndexia:CODISNumberText" minOccurs="0"/>
					<xsd:element ref="ndexia:IBISNumberText" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ExplosiveAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type that supplements nc:ExplosiveType.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://fbi.gov/cjis/N-DEx/IncidentArrest/2.1" i:name="AugmentationType"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ndexia:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="nc:ItemQuantity" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ExplosiveType">
		<xsd:annotation>
			<xsd:documentation>A data type for a set of additional information for Explosive in the LEXS Digest.</xsd:documentation>
			<xsd:appinfo>
				<i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="ExplosiveType"/>
			</xsd:appinfo>
		</xsd:annotation>
