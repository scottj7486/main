		</xsd:complexContent>
	</xsd:complexType>
<!-- Extension -->
	<xsd:complexType name="CaseAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about a case.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ccq-res-ext:CourtCaseSealedIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:RemandDate" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:UnderAdvisementDate" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:JuryRequestedIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:JuryVerdictIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:CourtCase"  minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:DefendantCaseFinancials" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="nc:SupervisionCustodyStatus" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:Attorney" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:CaseJudge" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:DefendantCase"  minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:Defendant"  minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CourtCaseType">
	
	<!-- Use nc:CourtCaseAugmentationPoint -->
	
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about a court case.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="nc:ActivityType">
				<xsd:sequence>
					<xsd:element ref="ccq-res-ext:FirstAppearanceDate" />
					<xsd:element ref="ccq-res-ext:TrialByText" />
					<xsd:element ref="ccq-res-ext:SpeedyTrialText" />
					<xsd:element ref="ccq-res-ext:FTPHoldIndefiniteIndicator" />
					<xsd:element ref="ccq-res-ext:FTAHoldDate" />
					<xsd:element ref="ccq-res-ext:FTPHoldDate" />
					<xsd:element ref="ccq-res-ext:FTCHoldDate" />
					<xsd:element ref="ccq-res-ext:FTPVHoldDate" />
					<xsd:element ref="ccq-res-ext:FPCNumberText"/>
					<xsd:element ref="j:CaseCourtEvent" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DefendantCaseFinancialType">
		<xsd:annotation>
			<xsd:documentation>A data type for a defendant's financial information within a court case.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="nc:AmountType">
				<xsd:sequence>
					<xsd:element ref="j:FinancialAccountPresentBalanceAmount" />
					<xsd:element ref="ccq-res-ext:BalanceDueDate" />
					<xsd:element ref="ccq-res-ext:BalanceExtensionDate" />
					<xsd:element ref="ccq-res-ext:DoNotAllowEPaymentIndicator" />
					<xsd:element ref="ccq-res-ext:InterestStartDate" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DefendantCaseType">
		<xsd:annotation>
			<xsd:documentation>A data type for a defendant's financial information within a court case.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="j:CourtActivityType">
				<xsd:sequence>
					<xsd:element ref="nc:IdentificationID" />
					<xsd:element ref="nc:CaseTrackingID" />
					<xsd:element ref="nc:PersonDescriptionText" />
					<xsd:element ref="j:CaseJudge" />
					<xsd:element ref="nc:DispositionDate" />
					<xsd:element ref="j:Violation" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DefendantType">
		<xsd:annotation>
			<xsd:documentation>A data type for a defendant that is related to a court case.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="j:SubjectType">
			
			<!--  Many of the properties below are properties on nc:person -->
			
			<!--  A number of the properties below are not consistent with a defendant or person properties -->
			
				<xsd:sequence>
					<xsd:element ref="nc:IdentificationID" />
					<xsd:element ref="nc:PersonName" />
					<xsd:element ref="nc:Organization" />
					<xsd:element ref="nc:SupervisionCustodyStatus" />
					<xsd:element ref="j:DriverLicense" />
					<xsd:element ref="ccq-res-ext:JuvenileIDText" />
					<xsd:element ref="ccq-res-ext:InmateIDText" />
					<xsd:element ref="nc:PersonOtherIdentification" />
					<xsd:element ref="biom:PhysicalFeatureDescriptionDetail" />
					<xsd:element ref="ccq-res-ext:DefendantCommentText" />
					<xsd:element ref="ccq-res-ext:ClerkCommentText" />
					<xsd:element ref="nc:PersonAliasIdentityAssociation" />
					<xsd:element ref="nc:LocationAddressAbstract" />
					<xsd:element ref="j:ContactPersonTelephoneNumber" />
					<xsd:element ref="nc:ElectronicAddress" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="LocationAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about a case.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ccq-res-ext:AddressIDText" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ElectronicAddressAugmentationType">
	
	<!-- Where is the element for this type declared ? -->
	
		<xsd:annotation>
			<xsd:documentation>A data type for additional information related to an electronic address.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ccq-res-ext:ElectronicContactIDText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:ElectronicContactTypeText" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CitationAugmentationType">
	
	<!--  Where is the element for this type declared ? -->
	
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about a citation.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ccq-res-ext:CitationIDText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:CourtAppearanceDate" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:DrivingIncidentLegalSpeedRateMeasure" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:DrivingIncidentRecordedSpeedRateMeasure" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:PoundsOverMeasure" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="nc:IncidentLocation" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:MustAppearIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:IncidentTrafficAccidentInvolvedIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:IncidentTrafficInjuryIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:IncidentSchoolZoneIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:MotorCarrierHazMatTransportationIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:NuclearMaterialIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:CaseDomesticViolenceIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:DriverLicenseSurrenderedIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:IncidentTrafficFatalIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:IncidentConstructionZoneIndicator" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CourtEventAugmentationType">
	
		<!--  Where is the element for this type declared ? -->
	
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about a court event.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ccq-res-ext:JSICommentText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:HearingCategoryText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:HearingDecisionDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:MinuteOrderDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ChargeAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about a charge.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="j:ArrestProbableCauseText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:ArrestOfficial" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:EnforcementUnitName" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:PointNumberText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:PointsReducesNumberText"  minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:PersonBloodAlcoholContentNumberText"  minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:PersonBloodAlcoholContentTestRefusedIndicator"  minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:Violation"  minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:ArrestAgencyRecordIdentification"  minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:ChargePlea" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="BailBondAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about a bond.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ccq-res-ext:BondNumberText" minOccurs="0" maxOccurs="unbounded"/>					
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="WarrantAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about a court warrant.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ccq-res-ext:WarrantIDText" minOccurs="0" maxOccurs="unbounded"/>	
					<xsd:element ref="nc:ActivityDate" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="nc:ActivityDescriptionText" minOccurs="0" maxOccurs="unbounded"/>				
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>	
	<xsd:complexType name="PaymentType">
		<xsd:annotation>
			<xsd:documentation>A data type for payment information.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:ObjectType">
				<xsd:sequence>
					<xsd:element ref="ccq-res-ext:PaymentIDText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:BankruptIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:BadCheckIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:BadCheckPaidIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:BadAddressIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:PaymentLastPaidDate" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="VehicleAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about a vehicle.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ccq-res-ext:VehicleIDText" minOccurs="0" maxOccurs="unbounded"/>	
					<xsd:element ref="j:ConveyanceRegistrationPlateIdentification" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:ConveyanceColorPrimaryCode" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="nc:ItemDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="nc:ItemModelYearDate" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="nc:VehicleCMVIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:VehicleStyleCode" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="SentenceAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about a sentence.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ccq-res-ext:SentenceIDText" minOccurs="0" maxOccurs="unbounded"/>	
					<xsd:element ref="ccq-res-ext:DispositionIDText" minOccurs="0" maxOccurs="unbounded"/>	
					<xsd:element ref="ccq-res-ext:SentenceDeferredDate" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:SentenceConfinementComment" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="nc:FacilityDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:OtherSentence" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:Probation"	minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="OtherSentenceType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about a Sentence.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:ObjectType">
				<xsd:sequence>
					<xsd:element ref="ccq-res-ext:OtherSentenceIDText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:DispositionIDText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:ProgramTitleText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="nc:ProgramCondition" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ConditionAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about a program condition.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ccq-res-ext:ConditionDaysToCompleteText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:ConditionCompleteInText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:ConditionHoursAssignedText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:ConditionModificationDate" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:ConditionOtherHoursCompleteText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:ConditionOtherTermsText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:ProgramCostPerHourAmount" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="SupervisionAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about probation and supervision.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ccq-res-ext:DispositionIDText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:ProbationMinYearsText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:ProbationMinMonthsText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:ProbationMinDaysText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:ProbationActualDate" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:ProbationCompleteByDate" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="PoundsOverMeasureType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about a citation.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:ObjectType">
				<xsd:sequence>
					<xsd:element ref="nc:MeasureUnitText" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AddressAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about a citation.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:AugmentationType">
				<xsd:sequence>
					<xsd:element ref="ccq-res-ext:StreetTypeText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ccq-res-ext:ApartmentNumberText" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="VictimAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about a citation.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:AugmentationType">
				<xsd:sequence>
