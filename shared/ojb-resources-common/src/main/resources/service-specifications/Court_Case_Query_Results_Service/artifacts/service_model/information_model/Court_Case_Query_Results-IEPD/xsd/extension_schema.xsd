			<xsd:extension base="nc:DocumentType">
				<xsd:sequence>
					<!--  xsd:element ref="ccq-res-ext:SourceSystemNameText" minOccurs="1" maxOccurs="unbounded" />
					<xsd:element ref="nc:Case" minOccurs="0" maxOccurs="unbounded"/> 
					<xsd:element ref="ccq-res-ext:Defendant" minOccurs="0" maxOccurs="unbounded"/> 
					<xsd:element ref="j:ContactPersonLocation" minOccurs="0" maxOccurs="unbounded"/> 
					<xsd:element ref="j:Citation" minOccurs="0" maxOccurs="unbounded"/> 
					<xsd:element ref="j:Arrest" minOccurs="0" maxOccurs="unbounded"/> 
					<xsd:element ref="j:CaseCourtEvent" minOccurs="0" maxOccurs="unbounded"/> 
					<xsd:element ref="j:CaseHearing" minOccurs="0" maxOccurs="unbounded"/> 
					<xsd:element ref="j:Charge" minOccurs="0" maxOccurs="unbounded"/> 
					<xsd:element ref="nc:Supervision" minOccurs="0" maxOccurs="unbounded"/> 
					<xsd:element ref="j:BailBond" minOccurs="0" maxOccurs="unbounded"/> 
					<xsd:element ref="j:Warrant" minOccurs="0" maxOccurs="unbounded"/> 
					<xsd:element ref="ccq-res-ext:Payment" minOccurs="0" maxOccurs="unbounded"/> 
					<xsd:element ref="nc:Vehicle" minOccurs="0" maxOccurs="unbounded"/> 
					<xsd:element ref="j:ChargeDisposition" minOccurs="0" maxOccurs="unbounded"/> 
					<xsd:element ref="j:ChargeSentence" minOccurs="0" maxOccurs="unbounded"/> 
					<xsd:element ref="j:Victim" minOccurs="0" maxOccurs="unbounded"/-->
