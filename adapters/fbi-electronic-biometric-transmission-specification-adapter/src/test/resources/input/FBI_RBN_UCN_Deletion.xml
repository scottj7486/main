                </ansi-nist:ContentRecordSummary>
            </ansi-nist:TransactionContentSummary>
            <!--DCS 1.015, Optional -->
            <ansi-nist:TransactionCharacterSetDirectory>
                <ansi-nist:CharacterSetCommonNameCode>UTF-8</ansi-nist:CharacterSetCommonNameCode>
