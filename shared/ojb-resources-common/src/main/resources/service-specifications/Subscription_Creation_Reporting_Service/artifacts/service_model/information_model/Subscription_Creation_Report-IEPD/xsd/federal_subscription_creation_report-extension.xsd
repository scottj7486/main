<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:niem-xs="http://release.niem.gov/niem/proxy/xsd/3.0/" xmlns:structures="http://release.niem.gov/niem/structures/3.0/" xmlns:nc="http://release.niem.gov/niem/niem-core/3.0/" xmlns:appinfo="http://release.niem.gov/niem/appinfo/3.0/" xmlns:j="http://release.niem.gov/niem/domains/jxdm/5.0/" xmlns:fed_subcr-ext="http://ojbc.org/IEPD/Extensions/FederalSubscriptionCreationReportExtension/1.0" targetNamespace="http://ojbc.org/IEPD/Extensions/FederalSubscriptionCreationReportExtension/1.0" elementFormDefault="qualified">
	<xsd:import namespace="http://release.niem.gov/niem/niem-core/3.0/" schemaLocation="./Subset/niem/niem-core/3.0/niem-core.xsd"/>
	<xsd:import namespace="http://release.niem.gov/niem/domains/jxdm/5.0/" schemaLocation="./Subset/niem/domains/jxdm/5.0/jxdm.xsd"/>
	<xsd:import namespace="http://release.niem.gov/niem/proxy/xsd/3.0/" schemaLocation="./Subset/niem/proxy/xsd/3.0/xs.xsd"/>
	<xsd:import namespace="http://release.niem.gov/niem/structures/3.0/" schemaLocation="./Subset/niem/structures/3.0/structures.xsd"/>
	<xsd:complexType name="FederalSubscriptionCreationReportType">
		<xsd:annotation>
			<xsd:documentation>A data type for a set of details associated with the creation of a federal subscription.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="nc:DocumentType">
				<xsd:sequence>
					<xsd:element ref="fed_subcr-ext:RapBackSubscriptionData"/>
					<xsd:element ref="j:Subject"/>
					<xsd:element ref="nc:Person"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="RapBackSubscriptionDataType">
		<xsd:annotation>
			<xsd:documentation>A data type for details associated with a Rap Back subscription.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="structures:ObjectType">
				<xsd:sequence>
					<xsd:element ref="fed_subcr-ext:RapBackActivityNotificationFormatCode" minOccurs="0"/>
					<xsd:element ref="fed_subcr-ext:RapBackCategoryCode" minOccurs="0"/>
					<xsd:element ref="fed_subcr-ext:RapBackExpirationDate" minOccurs="0"/>
					<xsd:element ref="fed_subcr-ext:RapBackInStateOptOutIndicator" minOccurs="0"/>
					<xsd:element ref="fed_subcr-ext:RapBackSubscriptionDate" minOccurs="0"/>
					<xsd:element ref="fed_subcr-ext:RapBackSubscriptionIdentification" minOccurs="0"/>
					<xsd:element ref="fed_subcr-ext:RapBackSubscriptionTermCode" minOccurs="0"/>
					<xsd:element ref="fed_subcr-ext:RapBackTermDate" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="RBCCodeType">
		<xsd:annotation>
			<xsd:documentation>A data type for a code describing a rap back purpose category.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="fed_subcr-ext:RBCCodeSimpleType">
				<xsd:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:simpleType name="RBCCodeSimpleType">
		<xsd:annotation>
			<xsd:documentation>An enumeration set for Rap Back Category codes.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="F">
				<xsd:annotation>
					<xsd:documentation>Firearms</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="I">
				<xsd:annotation>
					<xsd:documentation>Volunteer, Child Care/School Employee, Non-Criminal Justice Employment and Licensing.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="J">
				<xsd:annotation>
					<xsd:documentation>Criminal Justice Employment</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CI">
				<xsd:annotation>
					<xsd:documentation>Criminal Justice Investigative</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CS">
				<xsd:annotation>
					<xsd:documentation>Criminal Justice - Supervision</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="S">
				<xsd:annotation>
					<xsd:documentation>Security Clearance Information Act</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="RBNFCodeType">
		<xsd:annotation>
			<xsd:documentation>A code type for the code indicating the type of rap back activity notification</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="fed_subcr-ext:RBNFCodeSimpleType">
				<xsd:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:simpleType name="RBNFCodeSimpleType">
		<xsd:annotation>
			<xsd:documentation>An enumeration set for a type of rap back activity notification.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="1">
				<xsd:annotation>
					<xsd:documentation>Pre-Notification</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="2">
				<xsd:annotation>
					<xsd:documentation>Triggering Event</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="3">
				<xsd:annotation>
					<xsd:documentation>Identity History Summary to include the triggering event</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="RBSubscriptionTermCodeType">
		<xsd:annotation>
			<xsd:documentation>A data type for the code indicating the rap back subscription term length.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="fed_subcr-ext:RBSubscriptionTermCodeSimpleType">
				<xsd:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:simpleType name="RBSubscriptionTermCodeSimpleType">
		<xsd:annotation>
			<xsd:documentation>A data type for an enumeration representation of code designations for a rap back subscription term.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="2">
				<xsd:annotation>
					<xsd:documentation>Two year subscription term</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="5">
				<xsd:annotation>
					<xsd:documentation>Five year subscription term</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="L">
				<xsd:annotation>
					<xsd:documentation>Lifetime subscription term</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="RapBackSubscriptionData" type="fed_subcr-ext:RapBackSubscriptionDataType">
		<xsd:annotation>
			<xsd:documentation>Details associated with a Rap Back subscription.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RapBackExpirationDate" type="nc:DateType">
		<xsd:annotation>
			<xsd:documentation>A date a Rap Back Subscription expires under the Subscriber's Privacy Strategy.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RapBackInStateOptOutIndicator" type="niem-xs:boolean">
		<xsd:annotation>
			<xsd:documentation>True if Submitter wishes to opt-out of receiving notifications for events originating within the submitterâ€™s own state; false otherwise.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RapBackSubscriptionDate" type="nc:DateType">
		<xsd:annotation>
			<xsd:documentation>The Date a rap back subscription was created.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RapBackSubscriptionIdentification" type="nc:IdentificationType">
		<xsd:annotation>
			<xsd:documentation>Unique identification assigned by NGI that identifies a rap back subscription as a whole.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RapBackSubscriptionTermCode" type="fed_subcr-ext:RBSubscriptionTermCodeType">
		<xsd:annotation>
			<xsd:documentation>A representation of the length of a Rap Back Subscription Term.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RapBackTermDate" type="nc:DateType">
		<xsd:annotation>
			<xsd:documentation>A date identifying the end of the fee paid by the Subscriber.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RapBackCategoryCode" type="fed_subcr-ext:RBCCodeType">
		<xsd:annotation>
			<xsd:documentation>A code indicating the category of the rap back subscription.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RapBackActivityNotificationFormatCode" type="fed_subcr-ext:RBNFCodeType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>A code indicating the type of rap back activity notification.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
</xsd:schema>
