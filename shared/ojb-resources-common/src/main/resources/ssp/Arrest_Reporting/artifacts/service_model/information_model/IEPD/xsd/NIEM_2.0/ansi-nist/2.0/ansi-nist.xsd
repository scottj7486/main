  <xsd:complexType name="PhysicalFeatureSizeType">
    <xsd:annotation>
      <xsd:documentation>DOCUMENTATION NOT PROVIDED</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
