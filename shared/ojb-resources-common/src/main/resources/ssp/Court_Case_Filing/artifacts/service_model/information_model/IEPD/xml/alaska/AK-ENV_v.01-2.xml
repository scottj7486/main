				<nc:ActivityDateRange>
					<nc:StartDate>
						<nc:Date>2016-10-18</nc:Date>
					</nc:StartDate>
					<nc:EndDate>
						<nc:Date>2016-10-19</nc:Date>
					</nc:EndDate>
				</nc:ActivityDateRange>
