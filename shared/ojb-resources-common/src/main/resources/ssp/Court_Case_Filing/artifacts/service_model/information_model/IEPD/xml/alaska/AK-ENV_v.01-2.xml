					<nc:IdentificationID>114624066</nc:IdentificationID>
					<nc:IdentificationCategoryText>ATN
					</nc:IdentificationCategoryText>
				</nc:ActivityIdentification>
				<nc:ActivityIdentification>
					<!--Charge Tracking Number (CTN) Required for the exchange -->
					<!--Stored in the PTYCHR05;ATN_TRK_NBR -->
					<nc:IdentificationID>041</nc:IdentificationID>
					<nc:IdentificationCategoryText>CTN
					</nc:IdentificationCategoryText>
				</nc:ActivityIdentification>
