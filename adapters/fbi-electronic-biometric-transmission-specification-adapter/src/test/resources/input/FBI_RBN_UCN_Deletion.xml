<itl:NISTBiometricInformationExchangePackage xmlns:ebts="http://cjis.fbi.gov/fbi_ebts/10.0"
    xmlns:ansi-nist="http://niem.gov/niem/biometrics/1.0"
    xmlns:itl="http://biometrics.nist.gov/standard/2011"
    xmlns:nc="http://niem.gov/niem/niem-core/2.0" xmlns:j="http://niem.gov/niem/domains/jxdm/4.1"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
