  <xsd:simpleType name="MaritalStatusCodeSimpleType">
    <xsd:annotation>
      <xsd:documentation>A data type for a code that identifies marital status.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="single">
        <xsd:annotation>
          <xsd:documentation>A person who is unmarried, or unattached to someone.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="married">
        <xsd:annotation>
          <xsd:documentation>A couple who lives together and is bound by a contract before the state or the church with legal age.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="separated">
        <xsd:annotation>
          <xsd:documentation>Legally bound despite a member of the couples not living with them, not in the household.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="divorced">
        <xsd:annotation>
          <xsd:documentation>A couple who previously lived together and was previously bound by a contract before the state or the church with legal age.  Excludes annulled marriages.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="widowed">
        <xsd:annotation>
          <xsd:documentation>A person whose spouse is dead.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="engaged">
        <xsd:annotation>
          <xsd:documentation>A person who has promised to give their hand in marriage to another person.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="annulled">
        <xsd:annotation>
          <xsd:documentation>A person whose previous marriage is considered to never have existed, been deemed null through declaration of nullity.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="cohabitating">
        <xsd:annotation>
          <xsd:documentation>A couple that is attached, yet unmarried with each other.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="unknown">
        <xsd:annotation>
          <xsd:documentation>Unknown</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
