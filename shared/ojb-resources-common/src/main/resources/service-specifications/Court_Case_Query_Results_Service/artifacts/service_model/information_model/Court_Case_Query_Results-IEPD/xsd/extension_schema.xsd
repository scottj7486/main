					<xsd:element ref="ccq-res-ext:VictimRestitutionHoldDate" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
