 <script type="text/javascript">
 
  $(function() {
  	$('#searchBarButtonDiv').hide();    		
  	
    $("#searchResultsTable").DataTable({
		 "pagingType": "full_numbers",
		 "pageLength": 25,
		 stateSave: true,
		 colReorder: true,
		 "language": {
      		"emptyTable": "No Federal Rapback Notifications found"
    	 } 
	});
	
	$("#subscriptionButtons").appendTo("#searchResultsTable_length");	  
	
	$('#searchForm').click (function() { 
		return adminPage.populateSubsAdminContentContentFromUrl("#springUrl('/subscriptions/admin/notificationsSearchForm')")
    });
	
  });
    
</script>
	
<div style="padding:10px">
	<span id="subscriptionButtons">
		<a id="searchForm" href="#" class="blueButton viewDetails"><img style="margin-right:6px;" src="../static/images/Search%20Detail/icon-close.png"/>SEARCH</a>
	</span>    				
	<h4>Federal Rapback Notifications</h4>
	<table class="searchResultsTable display" id="searchResultsTable">
		<thead>
			<tr>
				<th>STATE SUBSCRIPTION ID</th>
				<th>TRANSACTION TYPE</th>
				<th>EVENT TEXT</th>
				<th>TRIGGERING EVENTS</th>
				<th>DATETIME RECEIVED</th>
			</tr>
		</thead>
		<tbody>
	        #foreach($federalRapbackNotification in $federalRapbackNotifications)
		        <tr>
		        	<td>$federalRapbackNotification.stateSubscriptionId</td>
		        	<td>$federalRapbackNotification.transactionType</td>
		        	<td>$federalRapbackNotification.rapBackEventText</td>
		        	<td>$federalRapbackNotification.triggeringEventsString</td>
		        	<td>$federalRapbackNotification.notificationRecievedTimestamp.format($dateTimeformatter)</td>
	        	</tr>
	        #end
		</tbody>
	</table> 								
</div>
